{#
@see paginator_skeleton.html.twig
@param undefined|Object tableAttr - Attr key=>val para <table>
@param undefined|Object headAttr - Attr key=>val para <thead>
@param undefined|Object bodyAttr - Attr key=>val para <tbody>
@param undefined|Object footAttr - Attr key=>val para <tfoot>
#}
{% extends '@Dashboard/object/pager/paginator_skeleton.html.twig' %}
{%- set tableAttr= (tableAttr ?? {})|merge({
    'id':   tableAttr.id ?? tableId|default(''),
    'class':  'table table-bordered table-responsive-md mb-0 js-table table-hover'~(' '~tableAttr.class ?? '')~(' '~tableClass??''),
}) -%}
{%- set headAttr= (headAttr ?? {})|merge({
    'class':  'js-head'~(' '~headAttr.class ?? '')~(' '~headClass??''),
}) -%}
{%- set bodyAttr= (bodyAttr ?? {})|merge({
    'id':  bodyAttr.id ?? bodyId|default(''),
    'class':  'js-body'~(' '~bodyAttr.class ?? '')~(' '~bodyClass??'')
}) -%}
{%- set footAttr= (footAttr ?? {})|merge({
    'class':  'js-foot'~(' '~footAttr.class ?? '')~(' '~footClass??''),
}) -%}
{% block pagination %}
<table {% for key, value in tableAttr|filter(value => value != '') %} {{ key }}="{{ value|raw }}" {% endfor %} >
<thead {% for key, value in headAttr|filter(value => value != '') %} {{ key }}="{{ value|raw }}" {% endfor %} >
<tr class="text-center-row bg-gray-200 ">
    {% block head %}
    {%  endblock head %}
    <th style="width: 0px"><i class="fas fa-sync fa-sm mr-2 js-sync"></i></th>
</tr>
</thead>
<tbody {% for key, value in bodyAttr|filter(value => value != '') %} {{ key }}="{{ value|raw }}" {% endfor %} >
</tbody>
<tfoot {% for key, value in footAttr|filter(value => value != '') %} {{ key }}="{{ value|raw }}" {% endfor %}>
{% block foot %}
{%  endblock foot %}
</tfoot>
</table>
{% endblock  pagination%}
{% block templateBody %}
    <tr role="row">
        {% block script %}
        {%  endblock script %}
        <td class="text-center">{% include '@Dashboard/object/template/manytoone_action_buttons.html.twig'%}</td>
    </tr>
{% endblock templateBody %}
