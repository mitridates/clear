{%- set attr= (attr ?? {})|merge({
    'class':  'btn btn-success btn-outline-default'~(' '~attr.class ?? '')~(' '~class??''),
    'href': href ?? attr.href|default('javascript:void(0);')
}) -%}
<a {% for key, value in attr %} {{ key }}="{{ value|raw }}" {% endfor %}><i class="fa fa-plus-circle"></i> {{ text|default('btn.new'|trans({},'cavemessages'))|raw -}}</a>